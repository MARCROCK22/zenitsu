{
    "compilerOptions": {
        "target": "ES6",
        "module": "esnext",
        "outDir": "./dist",
        "rootDir": "./src",
        "removeComments": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": false,
    }
}
